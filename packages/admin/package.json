  "scripts": {
    "build:prod": "next build",
    "check-all": "npm-run-all check-code  -p test",
    "check-code": "run-p lint spell-check type-check",
    "dev": "NODE_ENV=development run-p server:dev",
    "doc": "run-p doc:*",
    "doc:ui": "start-storybook -p 4001 --no-dll",
    "fix": "run-p fix:*",
    "fix:config": "prettier .eslintrc.js ./*.js ./.storybook/*.js --write",
    "fix:json": "prettier ./*.json --write",
    "fix:pack": "fixpack",
    "fix:ts": "eslint './src/**/*.{ts,tsx}' --fix",
    "lint": "run-p lint:*",
    "lint:css": "stylelint './src/**/*.{ts,tsx}'",
    "lint:ts": "eslint './src/**/*.{ts,tsx}'",
    "pre-commit": "lint-staged --config lint-staged.pre-commit.config.js",
    "pre-push": "lint-staged --config lint-staged.pre-push.config.js",
    "re-install": "lerna clean && lerna bootstrap",
    "server:dev": "nodemon",
    "server:prod": "ts-node --files ./src/bin/www/index.ts",
    "shared-items": "yarn remove shared-items; yarn add ../shared-items",
    "spell-check": "cspell './src/**/*.{ts,tsx}'",
    "start": "NODE_ENV=production run-s build:prod server:prod",
    "test": "jest --coverage",
    "test:report": "git add ./coverage/* && git commit -m \"test(admin): `date \"+%Y-%m-%d %H:%M:%S\"`\"",
    "type-check": "tsc"
  },
