{
  "name": "main-api",
  "version": "0.0.0",
  "author": "akky-xxxx <114.s.aki@gmail.com>",
  "private": true,
  "repository": "https://github.com/akky-xxxx/my-homepage.git",
  "scripts": {
    "check-all": "run-s check-code test",
    "check-code": "run-p spell-check lint type-check",
    "dev": "nodemon",
    "doc": "echo 'nothing task'",
    "fix": "run-p fix:*",
    "fix:config": "prettier .eslintrc.js ./*.js --write",
    "fix:json": "prettier ./*.json --write",
    "fix:pack": "fixpack",
    "fix:ts": "eslint './src/**/*.ts' --fix",
    "lint": "run-p lint:*",
    "lint:ts": "eslint './src/**/*.ts'",
    "pre-commit": "lint-staged --config lint-staged.pre-commit.config.js",
    "pre-push": "lint-staged --config lint-staged.pre-push.config.js",
    "re-install": "lerna clean && lerna bootstrap",
    "shared-items": "yarn remove shared-items; yarn add ../shared-items",
    "spell-check": "cspell './src/**/*.ts'",
    "test": "jest",
    "type-check": "tsc"
  },
  "dependencies": {
    "apollo-server-fastify": "^2.19.2",
    "dotenv": "^8.2.0",
    "faker": "^5.1.0",
    "fastify": "^3.10.1",
    "fastify-compress": "^3.4.1",
    "fastify-helmet": "^5.1.0",
    "remeda": "^0.0.27",
    "shared-items": "^0.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/faker": "^5.1.5",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.21",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "cspell": "^5.1.3",
    "eslint": "^7.18.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "fixpack": "^4.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "supertest": "^6.1.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "ts-node-paths": "^1.0.1",
    "typescript": "^4.1.3"
  }
}
