{
  "name": "admin-api",
  "version": "0.0.0",
  "author": "akky-xxxx <114.s.aki@gmail.com>",
  "private": true,
  "repository": "https://github.com/akky-xxxx/my-homepage.git",
  "scripts": {
    "check-all": "run-s check-code test",
    "check-code": "run-p spell-check lint type-check",
    "dev": "run-p serve:*",
    "doc": "run-p doc:*",
    "doc:api": "agreed-ui --path ./spec/agreed/index.ts --port 4102",
    "fix": "run-p fix:*",
    "fix:agreed": "eslint './spec/**/*.ts' --fix",
    "fix:config": "prettier .eslintrc.js ./*.js --write",
    "fix:json": "prettier ./*.json --write",
    "fix:pack": "fixpack",
    "fix:ts": "eslint './src/**/*.ts' --fix",
    "lint": "run-p lint:*",
    "lint:agreed": "eslint './spec/**/*.ts'",
    "lint:ts": "eslint './src/**/*.ts'",
    "pre-commit": "lint-staged --config lint-staged.pre-commit.config.js",
    "pre-push": "lint-staged --config lint-staged.pre-push.config.js",
    "re-install": "lerna clean && lerna bootstrap",
    "serve:api": "nodemon",
    "serve:mock": "agreed-server --path ./spec/agreed/index.ts --port 4101 -l",
    "shared-items": "yarn remove shared-items; yarn add ../shared-items",
    "spell-check": "cspell './src/**/*.ts'",
    "test": "IS_TEST=true jest --detectOpenHandles",
    "type-check": "tsc"
  },
  "dependencies": {
    "@google-cloud/datastore": "^6.3.1",
    "cli-table": "^0.3.4",
    "compression": "^1.7.4",
    "date-fns": "^2.17.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-list-endpoints": "^5.0.0",
    "faker": "^5.3.1",
    "helmet": "^4.4.1",
    "remeda": "^0.0.27",
    "shared-items": "^0.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@agreed/cli": "^5.2.0",
    "@agreed/typed": "^5.2.0",
    "@types/cli-table": "^0.3.0",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.11",
    "@types/express-list-endpoints": "^4.0.1",
    "@types/faker": "^5.1.6",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.25",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "cspell": "^5.2.4",
    "eslint": "^7.19.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "fixpack": "^4.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.0",
    "ts-node": "^9.1.1",
    "ts-node-paths": "^1.0.1",
    "typescript": "^4.1.3"
  }
}
