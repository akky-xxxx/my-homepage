{
  "name": "shared-items",
  "version": "0.0.0",
  "author": "akky-xxxx <114.s.aki@gmail.com>",
  "private": true,
  "repository": "https://github.com/akky-xxxx/my-homepage.git",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "yarn check-all && yarn clean && tsc -P ./tsconfig.build.json",
    "check-all": "npm-run-all -p spell-check lint type-check -s test",
    "clean": "rimraf ./dist",
    "doc": "echo 'nothing task'",
    "doc:ui": "start-storybook -p 9999 --no-dll",
    "fix": "run-p fix:*",
    "fix:config": "prettier .eslintrc.js ./*.js --write",
    "fix:json": "prettier ./*.json --write",
    "fix:pack": "fixpack",
    "fix:ts": "eslint './src/**/*.ts' --fix",
    "lint": "run-p lint:*",
    "lint:ts": "eslint './src/**/*.ts'",
    "pre-commit": "lint-staged --config lint-staged.pre-commit.config.js",
    "pre-push": "lint-staged --config lint-staged.pre-push.config.js",
    "spell-check": "cspell './src/**/*.ts'",
    "test": "jest",
    "type-check": "tsc"
  },
  "dependencies": {
    "log4js": "^6.3.0"
  },
  "peerDependencies": {
    "date-fns": "^2.16.1",
    "log4js": "^6.3.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@storybook/addon-actions": "^6.1.9",
    "@storybook/addon-essentials": "^6.1.9",
    "@storybook/addon-links": "^6.1.9",
    "@storybook/react": "^6.1.9",
    "@types/jest": "^26.0.16",
    "@types/node": "^14.14.10",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/styled-components": "^5.1.4",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "babel-loader": "^8.2.2",
    "cspell": "^4.2.5",
    "eslint": "^7.15.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.0.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "fixpack": "^3.0.6",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "stylelint": "^13.8.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.1.2",
    "stylelint-processor-styled-components": "^1.10.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.0",
    "ts-node-paths": "^1.0.1",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.1.2"
  }
}
