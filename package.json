{
  "name": "portfolio",
  "version": "0.0.0",
  "author": "Shogo Akimoto <114.s.aki@gmail.com>",
  "license": "MIT",
  "repository": "https://github.com/akky-xxxx/portfolio.git",
  "main": "index.js",
  "scripts": {
    "build": "run-p build:*",
    "build:client": "next build ./src/client",
    "build:server": "parcel build src/server/www.ts -d bin --target node",
    "check-all": "run-p lint type-check spell-check",
    "convert-gallery": "ts-node scripts/convertGalleryTask.ts",
    "dev": "npm-run-all build -p serve",
    "docker": "run-s docker:build docker:run",
    "docker:build": "docker build ./ -t portfolio/fe",
    "docker:run": "docker run -p 30000:3000 -d portfolio/fe",
    "fix": "run-p fix:*",
    "fix:client": "eslint -c src/client/.eslintrc.js --ext .ts,.tsx ./src/client --fix",
    "fix:pack": "fixpack",
    "fix:scripts": "eslint -c ./scripts/.eslintrc.js --ext .ts,.tsx ./scripts --fix",
    "gcloud": "run-s gcloud:change:prj gcloud:submit gcloud:deploy",
    "gcloud:change:prj": "gcloud config set project eloquent-figure-263607",
    "gcloud:deploy": "gcloud beta run deploy --image gcr.io/eloquent-figure-263607/fe",
    "gcloud:submit": "gcloud builds submit --tag gcr.io/eloquent-figure-263607/fe",
    "lint": "run-p lint:*",
    "lint:client": "eslint -c src/client/.eslintrc.js --ext .ts,.tsx ./src/client",
    "lint:scripts": "eslint -c ./scripts/.eslintrc.js --ext .ts,.tsx ./scripts",
    "serve": "run-p serve:*",
    "serve:bff": "cross-env DEBUG=app:server:* node bin/www.js",
    "spell-check": "cspell './src/**/*.{ts,tsx}'",
    "type-check": "run-p type-check:*",
    "type-check:client": "tsc -p ./src/client",
    "type-check:scripts": "tsc -p ./scripts",
    "type-check:server": "tsc -p ./src/server"
  },
  "dependencies": {
    "@material-ui/core": "^4.8.3",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/styles": "^4.8.2",
    "@reduxjs/toolkit": "^1.2.1",
    "agreed-typed": "^1.0.1",
    "cacheable-response": "^1.10.0",
    "chalk": "^3.0.0",
    "cross-env": "^6.0.3",
    "date-fns": "^2.9.0",
    "express": "^4.17.1",
    "next": "^9.1.7",
    "next-routes": "^1.4.2",
    "nprogress": "^0.2.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-slick": "^0.25.2",
    "react-virtualized-auto-sizer": "^1.0.2",
    "react-window": "^1.8.5",
    "react-window-infinite-loader": "^1.0.5",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "remeda": "^0.0.14",
    "screenfull": "^5.0.0",
    "styled-components": "^4.4.1",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.0",
    "@babel/plugin-proposal-optional-chaining": "^7.8.0",
    "@storybook/react": "^5.3.1",
    "@types/date-fns": "^2.6.0",
    "@types/express": "^4.17.2",
    "@types/fs-extra": "^8.0.1",
    "@types/imagemagick": "^0.0.30",
    "@types/jest": "^24.0.25",
    "@types/material-ui": "^0.21.7",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.5",
    "@types/react-slick": "^0.23.4",
    "@types/react-virtualized-auto-sizer": "^1.0.0",
    "@types/react-window": "^1.8.1",
