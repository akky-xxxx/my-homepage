    "@types/redux-actions": "^2.6.1",
    "@types/sharp": "^0.23.1",
    "@types/styled-components": "^4.4.2",
    "@types/uuid": "^3.4.6",
    "@typescript-eslint/eslint-plugin": "^2.15.0",
    "@typescript-eslint/parser": "^2.15.0",
    "cspell": "^4.0.44",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
