  "main": "index.js",
  "scripts": {
    "analyze": "NODE_ENV=analyze yarn build",
    "build:dev": "cross-env DEBUG=app:server:* nodemon src/server/www.ts",
    "check-all": "run-p lint type-check spell-check",
    "convert-gallery": "ts-node scripts/convertGalleryTask.ts",
    "dev": "npm-run-all build:dev",
    "docker": "run-s docker:build docker:run",
    "docker:build": "docker build ./ -t my-homepage/fe",
    "docker:run": "docker run -p 30000:3000 -d my-homepage/fe",
    "fix": "run-p fix:*",
    "fix:client": "eslint -c src/client/.eslintrc.js --ext .ts,.tsx ./src/client --fix",
    "fix:pack": "fixpack",
    "fix:scripts": "eslint -c ./scripts/.eslintrc.js --ext .ts,.tsx ./scripts --fix",
    "gcloud": "run-s gcloud:change:prj gcloud:submit gcloud:deploy",
    "gcloud:change:prj": "gcloud config set project eloquent-figure-263607",
    "gcloud:deploy": "gcloud beta run deploy test --image gcr.io/eloquent-figure-263607/fe --platform managed --region=asia-northeast1",
    "gcloud:submit": "gcloud builds submit --tag gcr.io/eloquent-figure-263607/fe",
    "lint": "run-p lint:*",
    "lint:client": "eslint -c src/client/.eslintrc.js --ext .ts,.tsx ./src/client",
    "lint:scripts": "eslint -c ./scripts/.eslintrc.js --ext .ts,.tsx ./scripts",
    "lint:style": "stylelint './src/client/**/*.{ts,tsx}'",
    "spell-check": "cspell './src/**/*.{ts,tsx}'",
    "type-check": "run-p type-check:*",
    "type-check:client": "tsc -p ./src/client",
    "type-check:scripts": "tsc -p ./scripts",
    "type-check:server": "tsc -p ./src/server"
  },
