  "main": "index.js",
  "scripts": {
    "analyze": "NODE_ENV=analyze yarn build",
    "build:prod": "run-s build:prod:client",
    "build:prod:client": "next build ./src",
    "check-all": "npm-run-all -p lint type-check spell-check -s test",
    "convert-gallery": "ts-node scripts/convertGalleryTask.ts",
    "dev": "npm-run-all serve:dev",
    "docker": "run-s docker:build docker:run",
    "docker:build": "docker build ./ -t my-homepage/fe",
    "docker:run": "docker run -p 30000:3000 -d my-homepage/fe",
    "fix": "run-p fix:*",
    "fix:client": "eslint --ext .ts,.tsx ./src --fix",
    "fix:json": "run-p fix:json:*",
    "fix:json:1": "prettier ./*.json --write",
    "fix:json:2": "prettier ./**/*.json --write",
    "fix:pack": "fixpack",
    "fix:scripts": "eslint -c ./scripts/.eslintrc.js --ext .ts ./scripts --fix",
    "fix:server": "eslint --ext .ts ./server --fix",
    "gcloud": "run-s gcloud:change:prj gcloud:submit gcloud:deploy",
    "gcloud:change:prj": "gcloud config set project eloquent-figure-263607",
    "gcloud:deploy": "gcloud beta run deploy test --image gcr.io/eloquent-figure-263607/fe --platform managed --region=asia-northeast1",
    "gcloud:submit": "gcloud builds submit --tag gcr.io/eloquent-figure-263607/fe",
    "lint": "run-p lint:*",
    "lint:client": "eslint --ext .ts,.tsx ./src",
    "lint:scripts": "eslint -c ./scripts/.eslintrc.js --ext .ts ./scripts",
    "lint:server": "eslint --ext .ts ./server",
    "lint:style": "stylelint './src/**/*.{ts,tsx}'",
    "serve:dev": "nodemon server/www.ts",
    "serve:propd": "NODE_ENV=production ts-node server/www.ts",
    "spell-check": "cspell './src/**/*.{ts,tsx}'",
    "start": "npm-run-all build:prod serve:propd",
    "test": "run-s test:*",
    "test:e2e": "NODE_ENV=test jest --runInBand --config jest.config.e2e.js",
    "test:it": "NODE_ENV=test jest --config jest.config.it.js",
    "type-check": "run-p type-check:*",
    "type-check:client": "tsc -p ./src",
    "type-check:scripts": "tsc -p ./scripts",
    "type-check:server": "tsc -p server"
  },
